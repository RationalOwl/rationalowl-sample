<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.RationalOwlUMSDemo" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryVariant">@color/green_dark</item>
        <item name="colorOnPrimary">@color/white</item>

        <item name="colorSecondary">@color/blue</item>
        <item name="colorSecondaryVariant">@color/blue_light</item>
        <item name="colorOnSecondary">@color/white</item>

        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.RationalOwlUMSDemo.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>

    <style name="Theme.RationalOwlUMSDemo.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RationalOwlUMSDemo.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="ThemeOverlay.AppTheme.TextInputEditText.Outlined.Accent" parent="">
        <item name="colorPrimary">?attr/colorSecondary</item>
    </style>

    <style name="ThemeOverlay.AppTheme.Accent" parent="">
        <item name="colorPrimary">?attr/colorSecondary</item>
    </style>

    <style name="ThemeOverlay.AppTheme.Button.ElevatedButton.Accent" parent="Widget.Material3.Button.ElevatedButton">
        <item name="colorPrimary">?attr/colorSecondary</item>
    </style>

    <style name="ThemeOverlay.AppTheme.Button.ElevatedButton.Icon.Accent" parent="Widget.Material3.Button.ElevatedButton.Icon">
        <item name="colorPrimary">?attr/colorSecondary</item>
    </style>
</resources>