<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" 
	android:id="@+id/RelativeLayout01" 
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent"
	android:background="@drawable/main_backimg_1">
	<ImageView 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_centerInParent="true" 
		android:id="@+id/padTop" 
		android:layout_below="@layout/lock">
	</ImageView>
	<ImageView 
		android:id="@+id/ImageView01" 
		android:layout_centerHorizontal="true" 
		android:src="@drawable/lock" 
		android:scaleType="fitXY" 
		android:layout_width="90dip" 
		android:layout_above="@+id/padTop" 
		android:layout_height="100dip">
	</ImageView>
	<TextView 
		android:layout_centerHorizontal="true" 
		android:gravity="center_horizontal" 
		android:id="@+id/lockInfo" 
		android:text="" 
		android:layout_below="@+id/padTop" 
		android:layout_height="wrap_content" 
		android:paddingTop="10dip" 
		android:paddingLeft="10dip"
		android:paddingRight="10dip"
		android:paddingBottom="10dip"
		android:layout_width="fill_parent" 
		android:textSize="24sp">
	</TextView>
	<TextView 
		android:layout_centerHorizontal="true" 
		android:layout_height="wrap_content" 
		android:id="@+id/tel" 
		android:layout_below="@+id/lockInfo" 
		android:text="" 
		android:gravity="center_horizontal" 
		android:layout_width="fill_parent" 
		android:textColor="#ffff00" 
		android:textSize="22sp">
	</TextView>	
	
	<LinearLayout		
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"		
   		android:gravity="center_horizontal"   		
		android:layout_alignParentBottom="true"				
		>			
		
	</LinearLayout>
	
</RelativeLayout>
